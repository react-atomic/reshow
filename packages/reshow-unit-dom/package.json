{
  "version": "0.2.0",
  "name": "reshow-unit-dom",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-atomic/reshow",
    "directory": "packages/reshow-unit-dom"
  },
  "homepage": "https://github.com/react-atomic/reshow/tree/main/packages/reshow-unit-dom",
  "description": "TODO: description",
  "keywords": [],
  "author": "Hill <hill@kimo.com>",
  "license": "ISC",
  "dependencies": {
    "@types/mocha": "*",
    "chai": "*",
    "global-jsdom": "*",
    "jsdom": "^19.0.0",
    "mocha": "^9.x",
    "nyc": "*",
    "prettier": "*",
    "prettier-eslint": "*",
    "prettier-eslint-cli": "*",
    "reshow-constant": "*",
    "sinon": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.x"
  },
  "exports": {
    "require": "./build/cjs/src/index.js",
    "import": "./build/es/src/index.mjs"
  },
  "main": "./build/cjs/src/index.js",
  "module": "./build/es/src/index.mjs",
  "scripts": {
    "clean": "find ./build ./types -name '*.*' | xargs rm -rf",
    "build:cjs": "BABEL_ENV=cjs babel src -d build/cjs/src --ignore /**/__tests__ --root-mode upward",
    "build:es": "BABEL_ENV=es babel src -d build/es/src --out-file-extension .mjs --root-mode upward",
    "build:type": "npx -p typescript tsc src/*.js --jsx react-jsx --declaration --allowJs --emitDeclarationOnly --skipLibCheck --declarationDir types",
    "build": "npm run clean && npm run build:cjs && npm run build:es && npm run build:type",
    "mochaFor": "mocha -r global-jsdom/register",
    "mocha": "npm run mochaFor -- 'build/es/**/__tests__/*.mjs'",
    "test": "npm run build && npm run mocha",
    "prepublishOnly": "npm run test"
  },
  "types": "./types/index.d.ts",
  "files": [
    "types",
    "build",
    "package.json",
    "README.md"
  ],
  "engines": {
    "node": ">=14"
  }
}
