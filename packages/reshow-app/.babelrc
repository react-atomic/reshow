{
    "env": {
        "build": {
            "presets": [
              ["env", {"loose": true, "modules": false}],
              "react",
              "stage-0"
            ],
            "plugins": [
              "transform-object-assign",
              "transform-react-constant-elements",
              ["transform-react-remove-prop-types", {"mode": "wrap"}]
            ]
        },
        "production": {
            "presets": [
              ["env", {"loose": true, "modules": false}],
              "react",
              "stage-0"
            ],
            "plugins": [
              "transform-object-assign",
              "transform-react-constant-elements",
              "transform-react-pure-class-to-function",
              ["transform-react-remove-prop-types", {"mode": "wrap"}]
            ]
        }
    }
}
