{
  "description": "Reshow App Files Develop package",
  "repository": "react-atomic/reshow",
  "keywords": [
    "reactjs"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.x",
    "babel-core": "^6.x",
    "babel-plugin-transform-react-constant-elements": "*",
    "babel-plugin-transform-react-remove-prop-types": "*",
    "babel-preset-env": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-0": "*"
  },
  "scripts": {
    "clean": "find ./build -name '*.*' | xargs rm -rf",
    "copy": "cp package.json.build ./build/package.json && cp README.md ./build && cp .npmignore ./build && cp .babelrc ./build",
    "build:babel": "BABEL_ENV=build babel src -d build",
    "build": "npm run clean && npm run build:babel && npm run copy && cd build && yarn install",
    "prepublish": "npm run build"
  }
}
